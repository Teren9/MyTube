<%= form_for video, html: { multipart: true } do |f| %>
  <%= f.label :title %><br />
  <%= f.text_field :title %><br />
  <%= f.label :attachment %><br />
  <%= f.file_field :attachment %><br />
  <%= f.label :description %><br />
  <%= f.text_area :description %><br />
  <%= f.submit "Save" %><br />
<% end %>
